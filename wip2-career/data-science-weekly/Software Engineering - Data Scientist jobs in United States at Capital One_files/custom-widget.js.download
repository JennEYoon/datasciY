try {
    //To removes the tracking code with #symbol when user sharing the jobs through addthis widget.
    var flag = false;
    $("script[src]").each(function (data, value) {
        if ($(value).attr("src").match(/addthis_widget/i) != null) {
            flag = true;
        }
    })

    if (flag) {
        var addthis_config = addthis_config || {};
        addthis_config.data_track_addressbar = false;
        addthis_config.data_track_clickback = false;
    }
} catch (e) {
}
